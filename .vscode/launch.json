{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gatsby develop",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
            "outputCapture": "std",
            "console": "integratedTerminal",
            "args": [
                "develop"
            ],
            "stopOnEntry": false,
            "runtimeArgs": [
                "--nolazy"
            ],
            "sourceMaps": false,
        },
    ]
}